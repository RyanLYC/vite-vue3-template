{
  "name": "vite-vue3-template",
  "version": "0.1.1",
  "private": false,
  "description": "A Vite + Vue3 + TypeScript + Element Plus + SASS + ESLint + Prettier + 支持 jsx  支持setup 语法糖 TSTemplate starter",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-tsc": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "prepare": "husky install",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/RyanLYC/vite-vue3-template.git"
  },
  "keywords": [
    "Vite",
    "Vue3",
    "TypeScript",
    "Starter"
  ],
  "author": "lyc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RyanLYC/vite-vue3-template.git/issues"
  },
  "homepage": "https://github.com/RyanLYC/vite-vue3-template#readme",
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.6",
    "axios": "^0.21.1",
    "dayjs": "^1.10.7",
    "element-plus": "^2.2.6",
    "vue": "^3.2.20",
    "vuex": "^4.0.0",
    "vue-router": "^4.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.10.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/test-utils": "^2.0.0-rc.4",
    "autoprefixer": "^10.2.6",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^8.1.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.35.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.7.3",
    "vite": "^2.9.9",
    "vite-plugin-style-import": "^0.9.2",
    "vite-plugin-svg-icons": "^0.7.1",
    "vite-plugin-top-level-await": "^1.1.1",
    "vue-jest": "^5.0.0-alpha.7",
    "vue-tsc": "^0.0.15"
  },
  "directories": {
    "test": "tests"
  }
}
